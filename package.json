{
  "name": "skyblog",
  "version": "0.1.0",
  "private": true,
  "author": "xhp443@gmail.com",
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:type": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky",
    "predev": "npm run prisma:dev:generate && npm run script:api:runtime nodejs && npm run script:api:type",
    "prebuild": "npm run prisma:prod:generate && npm run script:api:runtime edge && npm run script:api:type",
    "prisma:dev:generate": "prisma generate --no-hints --schema prisma/sqlite",
    "prisma:dev:push": "prisma db push --schema prisma/sqlite",
    "prisma:prod:generate": "prisma generate --no-hints --schema prisma/psql",
    "prisma:prod:migrate": "dotenv -e .env.local -- prisma migrate dev --schema prisma/psql",
    "prisma:prod:migrate-create-only": "dotenv -e .env.local -- prisma migrate dev --create-only --schema prisma/psql",
    "prisma:prod:migrate-status": "dotenv -e .env.local -- prisma migrate status --schema prisma/psql",
    "script:api:type": "tsx scripts/generator-api-type.ts",
    "script:api:runtime": "tsx scripts/api-runtime.ts",
    "vercel:env:pull": "vercel env pull"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,css,json,yaml}": "prettier --write"
  },
