    "prisma:push": "prisma db push",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate-create-only": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "prisma:migrate-status": "dotenv -e .env.local -- prisma migrate status",
    "script:api:type": "tsx scripts/generator-api-type.ts",
    "script:api:runtime": "tsx scripts/api-runtime.ts",
    "vercel:env:pull": "vercel env pull"
  },
  "prisma": {
    "schema": "prisma"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,css,json,yaml}": "prettier --write",
    "*.prisma": "prisma format"
