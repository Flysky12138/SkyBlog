    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:type": "tsc --noEmit",
    "format": "prettier --write . && prisma format",
    "prepare": "husky",
    "predev": "npm run prisma:generate && npm run script:api:runtime nodejs && npm run script:api:type",
